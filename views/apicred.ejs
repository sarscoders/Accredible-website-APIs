<%- include('Header'); -%>
  <h1 class="text-center"> Profile </h1>
    <div class="container">
      <div class="row">
        <div class="col-sm-9 col-md-7 col-lg-5 mx-auto">
          <div class="card card-signin my-5">
            <div class="card-body">
              <h5 class="card-title text-center">Edit Credential</h5>
              <form class="form-signin" method="POST" >
                <div class="form-label-group">
                  <label for="inputEmail">Enter Your API Key</label>
                  <input type="text" id="inputapi" name="apikey" class="form-control" placeholder="Enter Your API Key" required autofocus>
                  
                </div>

                <div class="custom-control custom-checkbox mb-3 my-4 text-center">
                  <button class="btn btn-primary" type="submit">Submit</button>
                </div>
                
                <hr class="my-4">
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>


<script src="Bootstrap/js/bootstrap.bundle.min.js"></script>
</body>
</html>
